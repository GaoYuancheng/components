{
  "defaultSeverity": "error",
  "jsRules": {},
  "rulesDirectory": [],
  "extends": [
    "tslint-react",
    "tslint-eslint-rules"
  ],
  "rules": {
    "eofline": true,
    "no-console": false,
    "no-construct": true,
    "no-debugger": false,
    "no-reference": true,
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false,
    "jsx-alignment": false,
    "jsx-curly-spacing": false,
    "jsx-boolean-value": false,
    "jsx-wrap-multiline": false
  }
}
